{
  "name": "clickalicious/rng",
  "description": "Rng - The random number generator for PHP",
  "authors": [
    {
      "name": "Benjamin Carl",
      "email": "opensource@clickalicious.de",
      "homepage": "https://www.clickalicious.de",
      "role": "Developer"
    }
  ],
  "require": {
      "php": ">=5.3.0"
  },
  "require-dev": {
      "raulfraile/ladybug": "~1.0.9",
      "phpunit/phpunit": "~4.5.0"
  },
  "autoload": {
      "psr-4": {
          "Clickalicious\\": "src/Clickalicious/"
      }
  },
  "include-path": ["src/"],
  "minimum-stability": "dev",
  "keywords": [
    "RNG",
    "PRNG",
    "Random",
    "Randomize",
    "Number",
    "mersenne"
  ],
  "homepage": "https://github.com/clickalicious/Rng",
  "license": [
    "BSD-3-Clause"
  ],
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Rng/issues",
    "wiki": "https://github.com/clickalicious/Rng/wiki"
  },
  "config" : {
    "bin-dir" : "bin",
    "preferred-install": "dist",
    "platform": {
      "php": "5.3.9"
    }
  }
}
